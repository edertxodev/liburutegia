<table class="table table-bordered table-hover">
    <tr>
      <th><%= "Titulo" %></th>
      <th><%= "Fecha de registro" %></th>
    </tr>
      <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= link_to usuario.username, edit_user_path(:id) %></td>
      </tr>
      <% end %>
  </table>
  <%= will_paginate @usuarios, renderer: BootstrapPagination::Rails, :next_label => '&raquo;', :previous_label => '&laquo;' %>